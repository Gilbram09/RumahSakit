<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gilbram.rs">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rskhusus"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".about"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".detail.DetailHospitalActivity" />
        <activity android:name=".detail.DetailPuskesmasActivity" />
        <activity android:name=".Hospital.HospitalUmumActivity" />
        <activity android:name=".puskesmas.PuskesmasActivity" />
        <activity android:name=".Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.DetailPuskesmasActivity" />
        <activity android:name=".activities.DetailHospitalActivity" />
        <activity android:name=".activities.PuskesmasActivity" />
        <activity android:name=".activities.HospitalKhususActivity" />
        <activity android:name=".activities.HospitalUmumActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR API KEY" />
    </application>

</manifest>